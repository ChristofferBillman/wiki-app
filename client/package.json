{
  "name": "stocken-wiki",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:host": "vite --host",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "generate": "graphql-codegen",
    "generate:prod": "GRAPHQL_SCHEMA_URL=http://localhost/graphql graphql-codegen --config codegen.ts",
    "generate:dev": "graphql-codegen -w"
  },
  "graphql": {
    "schema": "http://localhost:3000/graphql",
    "documents": "**/*.{graphql,js,ts,jsx,tsx}"
  },
  "dependencies": {
    "@apollo/client": "^3.11.7",
    "@graphql-codegen/cli": "^5.0.2",
    "@graphql-codegen/client-preset": "^4.3.3",
    "@graphql-codegen/typescript-resolvers": "^4.2.1",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@parcel/watcher": "^2.4.1",
    "graphql": "^16.9.0",
    "mongoose": "^8.6.1",
    "react": "^18.2.0",
    "react-beautiful-dnd": "^13.1.1",
    "react-dom": "^18.2.0",
    "react-markdown": "^8.0.7",
    "react-router-dom": "^6.15.0"
  },
  "devDependencies": {
    "@types/node": "^20.16.5",
    "@types/react": "^18.2.15",
    "@types/react-beautiful-dnd": "^13.1.4",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "eslint": "^8.1.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "typescript": "^5.0.2",
    "vite": "^4.4.5"
  }
}
